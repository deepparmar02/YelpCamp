<% layout('layout/boilerplate') %>

    <body>
        <h1>All Campgrounds</h1>

        <div>
            <a href="/campground/new">New Campground</a>
        </div>

        <ul>
            <% for (let camp of camps){ %>
                <li>
                    <a href="/campground/<%= camp._id %>">
                        <%= camp.title %>
                    </a>
                </li>
                <% } %>
        </ul>